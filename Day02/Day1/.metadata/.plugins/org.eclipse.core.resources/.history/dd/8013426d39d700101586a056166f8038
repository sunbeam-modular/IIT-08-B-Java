package com.sunbeam;

import java.util.ArrayList;
import java.util.List;

public class Program {

	public static void main(String[] args) {
		List<String>list = new ArrayList<String>();
		list.add("India");
		list.add("USA");
		list.add("Germany");
		list.add("Iran");
		list.add("India");
		list.add("Finland");
		
		for(String s : list) {
			System.out.println(s.toString());
		}
		System.out.println();
		int index = 3; 
		list.add(index, "France");
		for(String s : list) {
			System.out.println(s.toString());
		}

	}

}
